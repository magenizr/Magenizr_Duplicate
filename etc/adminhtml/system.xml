        <section id="cms" translate="label" sortOrder="1000" showInDefault="1" showInWebsite="1" showInStore="1">
            <group id="magenizr_duplicate" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
